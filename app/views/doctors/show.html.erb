<%= @doctor.name %>
<%= @doctor.specialty %>
<%= @doctor.university %>

<%= @doctor.hospital.name %>

<% @doctor.patients.each do |patient| %>
  <section id="patient-<%= patient.id %>">
    <%= patient.name %>
    <%= button_to "Remove From Caseload", "/doctor_patients/#{patient.id}", method: :delete %>
  </section>
<% end %>
